<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ggc.epatter1.witnessfitness">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ExerciseListActivity"
            android:label="@string/title_activity_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ExerciseActivity"
            android:label="@string/title_activity_exercise_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".AboutScreen"
            android:label="@string/title_activity_about_screen"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".EditExerciseActivity"
            android:label="@string/title_activity_edit_exercise"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
